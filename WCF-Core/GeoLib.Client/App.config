<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/>
  </startup>

  <system.serviceModel>
    <client>
      <endpoint address="net.tcp://localhost:8009/GeoService" binding="netTcpBinding" contract="GeoLib.Contracts.IGeoService" name="tcpEP"></endpoint>
      <endpoint address="http://localhost/GeoService" binding="basicHttpBinding" contract="GeoLib.Contracts.IGeoService" name="httpEP"></endpoint>
      <endpoint address="http://localhost:9333/GeoService.svc/miguel" binding="wsHttpBinding" contract="GeoLib.Contracts.IGeoService" name="webEP"></endpoint>
    </client>
    <bindings>
      <netTcpBinding>
        <binding sendTimeout="00:00:05" maxReceivedMessageSize="2000000" receiveTimeout="00:00:05">
          <reliableSession inactivityTimeout="00:00:05" ordered="true" />
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>

</configuration>
